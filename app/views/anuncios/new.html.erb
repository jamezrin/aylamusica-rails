<h1><%= t('nuevo_anuncio') %></h1>
<%= form_for(@anuncio, local: true) do |form| %>
  <p>
    <%= form.label :url %><br>
    <%= form.url_field :url %>
  </p>

  <p>
    <%= form.label :imagen %><br>
    <%= form.file_field :imagen %>
  </p>

  <p>
    <%= form.label :correo %><br>
    <%= form.email_field :correo %>
  </p>

  <p>
    <%= form.label :posicion %>
    <%= form.number_field :posicion %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
